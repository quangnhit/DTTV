    "version": "0.2.0",
    "configurations": [
        {
            "name": "DTTV-DEV",
            "request": "launch",
            "program": "lib/main_dev.dart",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--target",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "DTTV-STG",
            "request": "launch",
            "program": "lib/main_stg.dart",
            "type": "dart",
            "args": [
                "--flavor",
                "stg",
                "--target",
                "lib/main_stg.dart"
            ]
        },
        {
            "name": "DTTV-PROD",
            "request": "launch",
            "program": "lib/main_prod.dart",
            "type": "dart",
            "args": [
                "--flavor",
                "prod",
                "--target",
                "lib/main_prod.dart"
            ]
        },
        {
            "name": "DTTV",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "DTTV (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "DTTV (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
